set multiplot layout 2,2 rowsfirst

set logscale y
set format y "%.1E"
set ylabel 'Residual'
set xlabel 'Iteration'
plot "< cat logout.out | grep 'Solving for Ux' | cut -d' ' -f9 | tr -d ','" title 'Ux' with lines,\
"< cat logout.out | grep 'Solving for Uy' | cut -d' ' -f9 | tr -d ','" title 'Uy' with lines,\
"< cat logout.out | grep 'Solving for omega' | cut -d' ' -f9 | tr -d ','" title 'omega' with lines,\
"< cat logout.out | grep 'Solving for k' | cut -d' ' -f9 | tr -d ','" title 'k' with lines,\
"< cat logout.out | grep 'Solving for p' | cut -d' ' -f9 | sed -n 'p;N' | tr -d ','" title 'p' with lines
 

unset logscale y
set format y "%.1f"
set ylabel 'Tmax (K)'
set xlabel 'Iteration'
plot "< cat logout.out | grep 'max(T)' | cut -d' ' -f7 | tr -d ','" title 'maxT' with lines


unset logscale y
set format y "%.1f"
set ylabel 'Umax (m/s)'
set xlabel 'Iteration'
plot "< cat logout.out | grep 'max(mag(U))' | cut -d' ' -f7 | tr -d ','" title 'maxU' with lines


unset logscale y
set format y "%.1f"
set ylabel '{/Symbol D}P (Pa)'
set xlabel 'Iteration'
plot "< cat logout.out | grep 'subtract(p)' | cut -d' ' -f7 | tr -d ','" title '{/Symbol D}P' with lines

unset multiplot


pause 10

reread
